{
    "builds": [
      {
        "src": "contio_backend/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "contio_backend/wsgi.py"
      }
    ],
    "functions": {
    "/api/email.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}